#! /bin/bash
#
# Last modified: 2013-03-21
#

# increase the number of file handles
ulimit -S -n 2000

# get the current directory without relying 
# on dirname which might not be installed
dir="${0%/*}"
cd "$dir"

AppBin=$dir/JavaMacLauncher.bin

# run the app from the same process
exec "$AppBin" "$@"

